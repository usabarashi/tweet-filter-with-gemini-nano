{
  "mcpServers": {
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest",
        "--autoConnect",
        "--channel=stable"
      ],
      "env": {}
    },
    "serena": {
      "command": "nix",
      "args": [
        "run",
        "github:oraios/serena",
        "--",
        "start-mcp-server",
        "--context",
        "claude-code",
        "--project-from-cwd",
        "--open-web-dashboard",
        "False"
      ]
    }
  }
}
